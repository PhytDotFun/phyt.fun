{
    "name": "@phyt/core",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "exports": {
        "./di": {
            "types": "./dist/di.d.ts",
            "import": "./dist/di.js"
        },
        "./contracts": {
            "types": "./dist/contracts.d.ts",
            "import": "./dist/contracts.js"
        }
    },
    "scripts": {
        "build": "rm -rf dist && bun build src/di.ts --outdir dist --target node --format esm --minify && bun build src/contracts.ts --outdir dist --target node --format esm --minify && tsc --declaration --emitDeclarationOnly --outDir dist",
        "check:types": "tsc --noEmit",
        "clean": "pnpm rimraf dist && pnpm rimraf node_modules && pnpm rimraf .turbo",
        "format": "prettier --write . --ignore-unknown",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "test": "vitest run"
    },
    "dependencies": {},
    "devDependencies": {
        "@phyt/eslint": "workspace:*",
        "@phyt/tsconfig": "workspace:*",
        "@privy-io/server-auth": "catalog:",
        "@types/node": "catalog:",
        "@types/pg": "catalog:",
        "bullmq": "catalog:",
        "drizzle-kit": "catalog:",
        "drizzle-orm": "catalog:",
        "eslint": "catalog:",
        "ioredis": "catalog:",
        "prettier": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:",
        "zod": "catalog:"
    }
}
