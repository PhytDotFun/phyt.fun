{
    "name": "@phyt/core",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "exports": {
        "./di": {
            "types": "./dist/di.d.ts",
            "default": "./dist/di.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "check:types": "tsc --noEmit",
        "clean": "pnpm rimraf dist && pnpm rimraf node_modules && pnpm rimraf .turbo",
        "format": "prettier --write . --ignore-unknown",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "test": "vitest run"
    },
    "dependencies": {
        "@privy-io/server-auth": "catalog:",
        "drizzle-orm": "catalog:",
        "ioredis": "catalog:"
    },
    "devDependencies": {
        "@phyt/tsconfig": "workspace:*",
        "@phyt/eslint": "workspace:*",
        "@types/node": "catalog:",
        "@types/pg": "catalog:",
        "drizzle-kit": "catalog:",
        "eslint": "catalog:",
        "tsup": "catalog:",
        "typescript": "catalog:",
        "prettier": "catalog:",
        "vitest": "catalog:"
    }
}
