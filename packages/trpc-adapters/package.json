{
    "name": "@phyt/trpc-adapters",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "exports": {
        "./trpc": {
            "types": "./dist/trpc.d.ts",
            "import": "./dist/trpc.js"
        },
        "./users/procedures": {
            "types": "./dist/users/procedures.d.ts",
            "import": "./dist/users/procedures.js"
        },
        "./users/repository": {
            "types": "./dist/users/repository.d.ts",
            "import": "./dist/users/repository.js"
        },
        "./users/service": {
            "types": "./dist/users/service.d.ts",
            "import": "./dist/users/service.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "check:types": "tsc --noEmit",
        "clean": "pnpm rimraf dist && pnpm rimraf node_modules && pnpm rimraf .turbo",
        "dev": "tsup --watch",
        "format": "prettier --write . --ignore-unknown",
        "lint": "eslint .",
        "lint:fix": "eslint --fix",
        "test": "vitest run"
    },
    "dependencies": {
        "@phyt/data-access": "workspace:*",
        "@privy-io/server-auth": "catalog:",
        "@t3-oss/env-core": "catalog:",
        "@trpc/server": "catalog:",
        "dotenv": "catalog:",
        "drizzle-orm": "catalog:",
        "superjson": "catalog:",
        "zod": "catalog:"
    },
    "devDependencies": {
        "@phyt/core": "workspace:*",
        "@phyt/eslint": "workspace:*",
        "@phyt/tsconfig": "workspace:*",
        "eslint": "catalog:",
        "tsup": "catalog:",
        "typescript": "catalog:",
        "vitest": "catalog:"
    }
}
